{:paths ["bb"]
 :deps  {tick/tick {:mvn/version "1.0"}}
 :tasks
 {:requires ([babashka.fs :as fs]
             [clojure.edn :as edn]
             [blog.tasks :as tasks])
  :init
  (do
    (defn log [& args]
      (apply println args))
    (defn shelll [& args]
      (apply println args)
      (apply shell args)))

  build (do
          (log "regenerating all pages")
          (tasks/regen-all-pages))
  watch (shelll "echo 'TODO impl watching'")
  serve (shelll "docsify serve docs")}}
